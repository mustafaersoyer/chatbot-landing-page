    "react": "17.0.1",
    "react-dom": "17.0.1",
    "react-facebook-login": "4.1.1",
    "react-ga": "3.3.0",
    "react-player": "2.9.0",
    "react-responsive-carousel": "3.2.22",
    "react-simply-carousel": "5.1.4",
    "react-tsparticles": "1.40.0",
